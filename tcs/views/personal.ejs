<body ng-app="myApp">
<div class="wthree_agile_admin_info">
		  <div class="w3_agileits_top_nav">
			<ul id="gn-menu" class="gn-menu-main">				<li class="gn-trigger">
					<a class="gn-icon gn-icon-menu"><i class="fa fa-bars" aria-hidden="true"></i><span>Menu</span></a>
					<nav class="gn-menu-wrapper">
						<div class="gn-scroller scrollbar1">
							<ul class="gn-menu agile_menu_drop">
								
								<li class="page">
							
						</div>
					</nav>
				</li>
                <li class="second logo"><h1><a href="#"><i class="fa fa-graduation-cap" aria-hidden="true"></i>TCS </a></h1></li>
					<li class="second admin-pic">
				       <ul class="top_dp_agile">
									<button type="button" class="btn btn-default btn-sm">
          								<span class="glyphicon glyphicon-log-out"></span> Log out
       								</button>
									
						</ul>
				</li>
		</div>
		<div class="clearfix"></div>
		
				<div class="inner_content">
				  
						<div class="w3l_agileits_breadcrumbs">
							<div class="w3l_agileits_breadcrumbs_inner">
								<ul>
								</ul>
							</div>
						</div>

					<div class="inner_content_w3_agile_info two_in" ng-controller="adminpersonalcontroller">
					  <h2 class="w3_inner_tittle">Sustainability & Wellness <form style="float: right">
							<input  type="search" name="search"  ng-model="user.username" placeholder="Search here..." required="">
							<button class="btn btn-default" type="submit"><i class="fa fa-search" aria-hidden="true" ng-click="getpersonaldata(user)"></i></button>
						</form></h2>
									<div class="agile-tables">
										<div class="w3l-table-info agile_info_shadow">
										 <h3 class="w3_inner_tittle two">Name : <span ng-bind="personal[0].username"></span></h3>
										 <h3 class="w3_inner_tittle two">Age :<span ng-bind="personal[0].age"></span></h3>
										 <h3 class="w3_inner_tittle two"> Gender :<span ng-if="personal[0].male==1" >M</span><span ng-if="personal[0].male!=1" >F</span></h3>
											<table id="table">
											<thead>
											  <tr>
												<th>Date & Time</th>
												<th>PrevalentHYP</th>
												<th>TotChol</th>
												<th>sysBP</th>
												<th>diaBP</th>
												<th>BMI</th>
												<th>HeartRate</th>
												<th>Glucose</th>
												<th>Breath Rate</th>
												<th>SpO2</th>
												<th>Diabetes</th>
												<th>Bronchiectasis</th>
												<th>CHD</th>
												<th>Hypoxemia</th>
												<th>Asthma</th>
											  </tr>
											</thead>
											<tbody>
											  <tr ng-repeat="user in personal">
												<td ng-bind="user.date + user.time"></td>
												<td ng-bind="user.prevalentHyp"></td>
												<td ng-bind="user.totChol"></td>
												<td ng-bind="user.sysBP"></td>
												<td ng-bind="user.diaBP"></td>
												<td ng-bind="user.BMI"></td>
												<td ng-bind="user.heartRate"></td>
												<td ng-bind="user.glucose"></td>
												<td ng-bind="user.BreathRate"></td>
												<td ng-bind="user.SpO2"></td>
												<td ng-bind="user.Diabetis"></td>
												<td ng-bind="user.Bronchie"></td>
												<td ng-bind="user.CHD"></td>
												<td ng-bind="user.Hypoxemia"></td>
												<td ng-bind="user.Asthma"></td>

											  </tr>
											</tbody>
										  </table>
									  </div>
							
				    </div>
				</div>

          <script type="text/javascript" src="js/jquery-2.1.4.min.js"></script>
		  <script src="js/modernizr.custom.js"></script>
		
		   <script src="js/classie.js"></script>
		  <script src="js/gnmenu.js"></script>
		  <script>
			new gnMenu( document.getElementById( 'gn-menu' ) );
		 </script>
<script type="text/javascript" src="js/jquery.basictable.min.js"></script>
<script type="text/javascript">
    $(document).ready(function() {
      $('#table').basictable();

      $('#table-breakpoint').basictable({
        breakpoint: 768
      });

      $('#table-swap-axis').basictable({
        swapAxis: true
      });

      $('#table-force-off').basictable({
        forceResponsive: false
      });

      $('#table-no-resize').basictable({
        noResize: true
      });

      $('#table-two-axis').basictable();

      $('#table-max-height').basictable({
        tableWrapper: true
      });
    });
</script>
<script src="js/screenfull.js"></script>
		<script>
		$(function () {
			$('#supported').text('Supported/allowed: ' + !!screenfull.enabled);

			if (!screenfull.enabled) {
				return false;
			}

			

			$('#toggle').click(function () {
				screenfull.toggle($('#container')[0]);
			});	
		});
		</script>
<script src="js/jquery.nicescroll.js"></script>
<script src="js/scripts.js"></script>

